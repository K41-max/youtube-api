  "components": {
    "securitySchemes": { "bearer": { "scheme": "bearer", "bearerFormat": "JWT", "type": "http" } },
    "schemas": {
      "VideoThumbnailDto": {
        "type": "object",
        "properties": {
          "quality": { "type": "string" },
          "url": { "type": "string" },
          "width": { "type": "number" },
          "height": { "type": "number" }
        },
        "required": ["quality", "url", "width", "height"]
      },
      "AuthorThumbnailDto": {
        "type": "object",
        "properties": {
          "url": { "type": "string" },
          "width": { "type": "number" },
          "height": { "type": "number" }
        },
        "required": ["url", "width", "height"]
      },
      "ChapterDto": {
        "type": "object",
        "properties": { "title": { "type": "string" }, "startTime": { "type": "number" } },
        "required": ["title", "startTime"]
      },
      "RecommendedVideoDto": {
        "type": "object",
        "properties": {
          "videoId": { "type": "string" },
          "title": { "type": "string" },
          "videoThumbnails": {
            "type": "array",
            "items": { "$ref": "#/components/schemas/VideoThumbnailDto" }
          },
          "author": { "type": "string" },
          "authorUrl": { "type": "string" },
          "authorId": { "type": "string" },
          "authorThumbnails": {
            "type": "array",
            "items": { "$ref": "#/components/schemas/AuthorThumbnailDto" }
          },
          "lengthSeconds": { "type": "number" },
          "viewCountText": { "type": "string" },
          "viewCount": { "type": "number" }
        },
        "required": [
          "videoId",
          "title",
          "videoThumbnails",
          "author",
          "authorUrl",
          "authorId",
          "authorThumbnails",
          "lengthSeconds",
          "viewCountText",
          "viewCount"
        ]
      },
      "VideoDto": {
        "type": "object",
        "properties": {
          "type": { "type": "string" },
          "title": { "type": "string" },
          "videoId": { "type": "string" },
          "videoThumbnails": {
            "type": "array",
            "items": { "$ref": "#/components/schemas/VideoThumbnailDto" }
          },
          "storyboards": { "type": "object" },
          "description": { "type": "string" },
          "descriptionHtml": { "type": "string" },
          "published": { "type": "number" },
          "publishedText": { "type": "string" },
          "keywords": { "type": "array", "items": { "type": "string" } },
          "viewCount": { "type": "number" },
          "likeCount": { "type": "number" },
          "paid": { "type": "boolean" },
          "premium": { "type": "boolean" },
          "isFamilyFriendly": { "type": "boolean" },
          "allowedRegions": { "type": "array", "items": { "type": "string" } },
          "genre": { "type": "string" },
          "genreUrl": { "type": "string" },
          "author": { "type": "string" },
          "authorId": { "type": "string" },
