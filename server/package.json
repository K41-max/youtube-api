{
  "name": "server",
  "main": "dist/main",
  "scripts": {
    "serve": "nest build --webpack --webpackPath webpack-hmr.config.cjs --watch",
    "dev": "pnpm run serve",
    "env:prod": "NODE_ENV=production",
    "build": "cross-env NODE_ENV=production nest build -c nest-cli.json --webpack --webpackPath webpack.config.cjs",
    "start": "pnpm run env:prod && node ./dist/main.cjs",
    "test": "jest",
    "lint": "eslint --ext .ts,.js,.vue .",
    "lint:fix": "eslint --ext .ts,.js,.vue --fix .",
    "gen:swagger": "pnpm run build && cross-env GENERATE_SWAGGER=true node ./dist/main.cjs",
    "gen:api": "pnpm run gen:swagger && openapi-typescript swagger-spec.json --output ../shared/api.schema.ts && rm -f swagger-spec.json"
  },
  "_moduleAliases": {
    "server": "dist/server"
  },
  "config": {
    "mongodbMemoryServer": {
      "runtimeDownload": false,
      "systemBinary": "/usr/bin/mongod"
    }
  },
  "dependencies": {
    "@fastify/cookie": "8.3.0",
    "@fastify/helmet": "10.0.1",
    "@fastify/multipart": "7.2.0",
